{
  "name": "kwdstudio",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run prebuild && parallelshell 'npm run server' 'npm run watch:css'",
    "server": "static .",
    "prebuild": "rm -rf javascripts stylesheets",
    "build": "npm run build:css",
    "build:css": "node-sass src/stylesheets -o stylesheets --include-path node_modules/sanitize.css --include-path node_modules/repetitivo/lib",
    "watch:css": "npm run build:css && npm run build:css -- -w"
  },
  "author": "Oktavilla",
  "license": "UNLICENSED",
  "dependencies": {
    "node-sass": "~4.5.0",
    "node-static": "~0.7.9",
    "parallelshell": "~2.0.0",
    "sanitize.css": "~4.1.0"
  }
}
